{
  "name": "universal-native-boilerplate",
  "version": "0.0.1",
  "author": "Charlie Hulcher",
  "private": true,
  "scripts": {
    "setup": "gulp setup",
    "start": "gulp -h",
    "start:packager": "node node_modules/react-native/local-cli/cli.js start",
    "start:packager:web": "webpack-dev-server --config web/webpack.config.js --inline --progress",
    "start:packager:macos": "node node_modules/react-native-macos/local-cli/cli.js start",
    "build:production:web": "cross-env NODE_ENV=production webpack --config web/webpack.config.js --progress",
    "build:debug:web": "webpack --config web/webpack.config.js --progress",
    "start:debug:server": "nodemon build/server/debug/main.js --ignore js/ --ignore web/ --ignore index.web.js",
    "start:production:web": "serve build/web/production -s -c 1",
    "test:native": "jest --config jest/jest.native.json",
    "test:web": "jest --config jest/jest.web.json",
    "test": "jest --config jest/jest.native.json && jest --config jest/jest.web.json",
    "lint": "eslint -c .eslintrc --fix js/**",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@blueprintjs/core": "^1.10.0",
    "@blueprintjs/datetime": "^1.8.0",
    "@blueprintjs/table": "^1.7.0",
    "d3-format": "^1.1.0",
    "d3-scale": "^1.0.4",
    "d3-time-format": "^2.0.3",
    "express": "^4.14.1",
    "file-saver": "^1.3.3",
    "fs-jetpack": "^0.6.4",
    "immutability-helper": "^2.1.2",
    "jsonp": "^0.2.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "nodemon": "^1.11.0",
    "offline-plugin": "^4.6.1",
    "react": "~15.4.0",
    "react-addons-css-transition-group": "^15.4.2",
    "react-d3-components": "^0.6.6",
    "react-dom": "^15.4.2",
    "react-native-i18n": "^1.0.0",
    "react-native-vector-icons": "^4.0.0",
    "react-native-web": "^0.0.70",
    "react-navigation": "^1.0.0-beta.3",
    "react-redux": "^5.0.2",
    "react-select": "^1.0.0-rc.3",
    "react-stockcharts": "^0.6.1",
    "redux": "^3.6.0",
    "redux-persist": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "redux-worker-middleware": "^1.0.0",
    "xlsx": "^0.9.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-remove-prop-types": "^1.0.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "favicons-webpack-plugin": "https://github.com/c-h-/favicons-webpack-plugin.git",
    "file-loader": "^0.10.0",
    "fs-extra": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.8",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "lodash.assign": "^4.2.0",
    "lodash.isequal": "^4.5.0",
    "path-exists": "^3.0.0",
    "react-test-renderer": "~15.4.0",
    "resource-hints-webpack-plugin": "^0.0.1",
    "run-sequence": "^1.2.2",
    "serve": "2.0.0",
    "shelljs": "^0.7.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "worker-loader": "^0.7.1",
    "yargs": "^6.6.0",
    "optimize-js-plugin": "^0.0.4",
    "open": "^0.0.5",
    "webpack-visualizer-plugin": "^0.1.10",
    "preload-webpack-plugin": "^1.1.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native-macos": {
    "react-native": "react-native-macos"
  }
}
